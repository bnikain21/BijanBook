{
  "expo": {
    "name": "budge-tracker",
    "slug": "budge-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/piggy_bank.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bijan.budgetracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/piggy_bank.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bijan.budgetracker",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "budge-tracker",
    "plugins": [
      "expo-sqlite",
      "expo-router",
      "@react-native-community/datetimepicker"
    ]
  }
}
